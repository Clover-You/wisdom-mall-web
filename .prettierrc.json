{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "eslintIntegration": true,
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "all",
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "preserve",
  "vueIndentScriptAndStyle": false,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": true
}
